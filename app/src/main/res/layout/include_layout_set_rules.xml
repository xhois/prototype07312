<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layoutSetRules"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    android:orientation="vertical">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@drawable/back_tabs">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button_back_to_summary"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_gravity="right"
            android:background="@drawable/custom_button2"
            android:drawableLeft="@drawable/ic_baseline_keyboard_backspace_24"
            android:drawableTint="@color/white"
            android:foregroundGravity="bottom"
            android:includeFontPadding="false"
            android:insetLeft="0dp"
            android:insetTop="0dp"
            android:insetRight="0dp"
            android:insetBottom="0dp"
            android:minWidth="0dp"
            android:minHeight="0dp"
            android:outlineProvider="none"
            android:paddingLeft="15dp"
            android:paddingTop="0dp"
            android:paddingRight="15dp"
            android:paddingBottom="0dp"
            android:text="활동요약으로 돌아가기"
            android:textColor="@color/white"
            android:textSize="15sp"
            app:iconGravity="end"
            app:layout_constraintEnd_toEndOf="@id/include_1"
            app:layout_constraintTop_toTopOf="@+id/include_1"
            tools:ignore="TouchTargetSizeCheck" />

        <LinearLayout
            android:layout_width="350dp"
            android:layout_height="match_parent"
            android:layout_marginTop="45dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/select2"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:background="@drawable/layerlist_back_select" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="350dp"
            android:layout_height="match_parent"
            android:layout_marginTop="45dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/item1_2"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="어플"
                android:textColor="@color/black" />

            <TextView
                android:id="@+id/item2_2"
                android:layout_width="90dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="사용시간"
                android:textColor="@color/textColor" />

            <TextView
                android:id="@+id/item3_2"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="웹"
                android:textColor="@color/textColor" />

            <TextView
                android:id="@+id/item4_2"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="전화"
                android:textColor="@color/textColor" />

            <TextView
                android:id="@+id/item5_2"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="위치"
                android:textColor="@color/textColor" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-0.1dp"
        android:elevation="-10dp"
        android:minWidth="0dp"
        android:minHeight="0dp">

        <include
            android:id="@+id/includeApp"
            layout="@layout/include2_item1_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible"   />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item2_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/back_item2"
            android:paddingBottom="30dp"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView2_2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="활동 타임라인"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView7_2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="자녀의 스마트기기에서 최근 어떤 활동이 일어 났는지 시간순서대로 나열합니다."
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView2_2" />

            <TextView
                android:id="@+id/textView8_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:paddingStart="10dp"
                android:paddingEnd="5dp"
                android:text="조회 시작일 :"
                android:textColor="@color/black"
                android:textStyle="normal|bold"
                app:layout_constraintEnd_toStartOf="@id/spinner_user2_2"
                app:layout_constraintTop_toTopOf="@id/spinner_user2_2" />

            <Spinner
                android:id="@+id/spinner_user2_2"
                android:layout_width="180dp"
                android:layout_height="35dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/convert_spinner_arrow2"
                android:contentDescription="11월15일,2021 - 11월15일,2021"
                android:popupBackground="@drawable/convert_spinner_arrow2"

                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7_2"
                tools:ignore="TouchTargetSizeCheck" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_usageTime_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/spinner_user2_2">

                <TextView
                    android:id="@+id/textView_usageTime_2"
                    android:layout_width="0dp"
                    android:layout_height="70dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time"
                    android:gravity="left|top"
                    android:paddingStart="10dp"
                    android:paddingTop="10dp"
                    android:text="타임라인"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/transparent"
                    android:orientation="horizontal"
                    android:paddingStart="5dp"
                    android:paddingEnd="5dp"
                    app:layout_constraintBottom_toBottomOf="@id/textView_usageTime_2"
                    app:layout_constraintStart_toStartOf="@id/textView_usageTime_2">


                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine1"
                        android:layout_width="65dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/custom_button4_left"
                        android:text="모든활동"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine2"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-0.5dp"
                        android:background="@drawable/custom_button4_hor"
                        android:text="앱"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine3"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-0.5dp"
                        android:background="@drawable/custom_button4_hor"
                        android:text="웹"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine4"
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-0.5dp"
                        android:background="@drawable/custom_button4_hor"
                        android:text="전화"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine5"
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-0.5dp"
                        android:background="@drawable/custom_button4_hor"
                        android:text="위치"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine6"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-0.5dp"
                        android:background="@drawable/custom_button4_right"
                        android:text="시스템"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />


                </LinearLayout>

                <ImageView
                    android:id="@+id/imageView_2"
                    android:layout_width="0dp"
                    android:layout_height="1140dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time_line_image"
                    android:padding="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_usageTime_2" />

                <TextView
                    android:id="@+id/textView_more_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:drawableLeft="@drawable/ic_reference_mark"
                    android:text="  로그가 보이지 않나요? 알아보기"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="@+id/imageView_2"
                    app:layout_constraintTop_toTopOf="@+id/imageView_2" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="58dp"
                    android:layout_marginTop="70dp"
                    android:layout_marginEnd="58dp"
                    android:background="@color/green"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:text="2021년 11월 15일 월요일"
                    android:textColor="@color/white"
                    app:layout_constraintEnd_toEndOf="@+id/imageView_2"
                    app:layout_constraintStart_toStartOf="@+id/imageView_2"
                    app:layout_constraintTop_toTopOf="@+id/imageView_2"
                    tools:ignore="TextContrastCheck" />

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="3dp"
                    android:layout_height="0dp"
                    android:layout_marginLeft="17dp"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="10dp"
                    app:dividerColor="#C1CBE4"
                    app:layout_constraintBottom_toBottomOf="@id/imageView_2"
                    app:layout_constraintStart_toStartOf="@id/imageView_2"
                    app:layout_constraintTop_toTopOf="@id/imageView_2" />

                <ImageView
                    android:id="@+id/imageView_timeline_1"
                    android:layout_width="330dp"
                    android:layout_height="180dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="@+id/imageView_2"
                    app:layout_constraintTop_toBottomOf="@+id/textView9"
                    app:srcCompat="@drawable/image_timeline_1" />


                <ImageView
                    android:id="@+id/imageView_timeline_2"
                    android:layout_width="330dp"
                    android:layout_height="180dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="0dp"
                    app:layout_constraintEnd_toEndOf="@+id/imageView_timeline_1"
                    app:layout_constraintTop_toBottomOf="@+id/imageView_timeline_1"
                    app:srcCompat="@drawable/image_timeline_2" />

                <ImageView
                    android:id="@+id/imageView_timeline_3"
                    android:layout_width="330dp"
                    android:layout_height="110dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="0dp"
                    app:layout_constraintEnd_toEndOf="@+id/imageView_timeline_2"
                    app:layout_constraintTop_toBottomOf="@+id/imageView_timeline_2"
                    app:srcCompat="@drawable/image_timeline_3" />

                <ImageView
                    android:id="@+id/imageView_timeline_4"
                    android:layout_width="330dp"
                    android:layout_height="390dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="0dp"
                    app:layout_constraintEnd_toEndOf="@+id/imageView_timeline_3"
                    app:layout_constraintTop_toBottomOf="@+id/imageView_timeline_3"
                    app:srcCompat="@drawable/image_timeline_4" />

                <ImageView
                    android:id="@+id/imageView_timeline_5"
                    android:layout_width="330dp"
                    android:layout_height="112dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="0dp"
                    app:layout_constraintEnd_toEndOf="@+id/imageView_timeline_4"
                    app:layout_constraintTop_toBottomOf="@+id/imageView_timeline_4"
                    app:srcCompat="@drawable/image_timeline_5" />

                <ImageView
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="33dp"
                    android:layout_marginRight="0dp"
                    app:layout_constraintEnd_toStartOf="@id/imageView_timeline_1"
                    app:layout_constraintTop_toTopOf="@id/imageView_timeline_1"
                    app:srcCompat="@drawable/ic_timeline_icon_1" />

                <ImageView
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="33dp"
                    android:layout_marginRight="0dp"
                    app:layout_constraintEnd_toStartOf="@id/imageView_timeline_2"
                    app:layout_constraintTop_toTopOf="@id/imageView_timeline_2"
                    app:srcCompat="@drawable/ic_timeline_icon_1" />

                <ImageView
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="33dp"
                    android:layout_marginRight="0dp"
                    app:layout_constraintEnd_toStartOf="@id/imageView_timeline_3"
                    app:layout_constraintTop_toTopOf="@id/imageView_timeline_3"
                    app:srcCompat="@drawable/ic_timeline_icon_1" />

                <ImageView
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="33dp"
                    android:layout_marginRight="0dp"
                    app:layout_constraintEnd_toStartOf="@id/imageView_timeline_4"
                    app:layout_constraintTop_toTopOf="@id/imageView_timeline_4"
                    app:srcCompat="@drawable/ic_timeline_icon_2" />

                <ImageView
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="33dp"
                    android:layout_marginRight="0dp"
                    app:layout_constraintEnd_toStartOf="@id/imageView_timeline_5"
                    app:layout_constraintTop_toTopOf="@id/imageView_timeline_5"
                    app:srcCompat="@drawable/ic_timeline_icon_3" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item3_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/back_item2"
            android:paddingBottom="30dp"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView2_3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="웹 화면"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item4_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/back_item2"
            android:paddingBottom="30dp"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView2_4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="전화 화면"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item5_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/back_item2"
            android:paddingBottom="30dp"
            android:visibility="gone">

            <TextView
                android:id="@+id/textView2_5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="위치 화면"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>

</LinearLayout>