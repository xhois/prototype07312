<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    android:orientation="vertical">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="@drawable/back_tabs">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button_setting_rull2"
            android:layout_width="110dp"
            android:layout_height="35dp"
            android:layout_gravity="right"
            android:background="@drawable/custom_button2"
            android:drawableLeft="@drawable/ic_baseline_settings_24"
            android:foregroundGravity="bottom"
            android:includeFontPadding="false"
            android:insetLeft="0dp"
            android:insetTop="0dp"
            android:insetRight="0dp"
            android:insetBottom="0dp"
            android:minWidth="0dp"
            android:minHeight="0dp"
            android:outlineProvider="none"
            android:paddingLeft="15dp"
            android:paddingTop="0dp"
            android:paddingRight="15dp"
            android:paddingBottom="0dp"
            android:text="규칙 설정"
            android:textColor="@color/white"
            android:textSize="15sp"
            app:iconGravity="end"
            app:layout_constraintEnd_toEndOf="@id/include_1"
            app:layout_constraintTop_toTopOf="@+id/include_1"
            tools:ignore="TouchTargetSizeCheck" />

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/select"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:background="@drawable/layerlist_back_select" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/item1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="활동 요약"
                android:textColor="@color/black" />

            <TextView
                android:id="@+id/item2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:drawableRight="@drawable/ic_timeline"
                android:drawablePadding="-15dp"
                android:gravity="center"
                android:paddingRight="15dp"
                android:text="타임라인"
                android:textColor="@color/textColor"
                app:drawableTint="@color/green" />

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-0.1dp"
        android:elevation="-10dp"
        android:minWidth="0dp"
        android:minHeight="0dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item1_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/back_item1"
            android:paddingBottom="30dp"
            android:visibility="visible">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="스마트기기 이용현황"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="자녀가 스마트기기에서 무엇을 했는지, 또 얼마나 많은 시간을 사용했는지 한눈에 보여줍니다.\n자녀의 활동 패턴을 파악하여 스마트기기를 올바르게 사용할 수 있도록 지도하세요."
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="조회할 날짜 선택:"
                android:textColor="@color/black"
                android:textStyle="normal|bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <Spinner
                android:id="@+id/spinner_user2"
                android:layout_width="280dp"
                android:layout_height="35dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/convert_spinner_arrow2"
                android:contentDescription="11월15일,2021 - 11월15일,2021"
                android:popupBackground="@drawable/convert_spinner_arrow2"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8"
                tools:ignore="TouchTargetSizeCheck" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_usageTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/spinner_user2">

                <TextView
                    android:id="@+id/textView_usageTime"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="이용 시간(분)"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/button_help"
                    android:layout_width="60dp"
                    android:layout_height="28dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/custom_button3"
                    android:foregroundGravity="bottom"
                    android:includeFontPadding="false"
                    android:insetLeft="0dp"
                    android:insetTop="0dp"
                    android:insetRight="0dp"
                    android:insetBottom="0dp"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:outlineProvider="none"
                    android:paddingLeft="7dp"
                    android:paddingTop="0dp"
                    android:paddingRight="7dp"
                    android:paddingBottom="0dp"
                    android:text="도움말"
                    android:textColor="@color/black"
                    android:textSize="13sp"
                    app:iconGravity="end"
                    app:layout_constraintBottom_toBottomOf="@+id/textView_usageTime"
                    app:layout_constraintEnd_toEndOf="@+id/textView_usageTime"
                    tools:ignore="TouchTargetSizeCheck" />

                <Spinner
                    android:id="@+id/spinner_today"
                    android:layout_width="90dp"
                    android:layout_height="28dp"

                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/convert_spinner_arrow3"
                    android:popupBackground="@drawable/convert_spinner_arrow3"
                    app:layout_constraintBottom_toBottomOf="@+id/textView_usageTime"
                    app:layout_constraintEnd_toStartOf="@+id/button_help"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="0dp"
                    android:layout_height="390dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time_image"
                    android:padding="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_usageTime"
                    app:srcCompat="@drawable/image_usagetime" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_usageCharacter"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layout_usageTime">

                <TextView
                    android:id="@+id/textView_usageCharacter"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="이용 성향"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/button_help2"
                    android:layout_width="60dp"
                    android:layout_height="28dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/custom_button3"
                    android:foregroundGravity="bottom"
                    android:includeFontPadding="false"
                    android:insetLeft="0dp"
                    android:insetTop="0dp"
                    android:insetRight="0dp"
                    android:insetBottom="0dp"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:outlineProvider="none"
                    android:paddingLeft="7dp"
                    android:paddingTop="0dp"
                    android:paddingRight="7dp"
                    android:paddingBottom="0dp"
                    android:text="도움말"
                    android:textColor="@color/black"
                    android:textSize="13sp"
                    app:iconGravity="end"
                    app:layout_constraintBottom_toBottomOf="@+id/textView_usageCharacter"
                    app:layout_constraintEnd_toEndOf="@+id/textView_usageCharacter"
                    tools:ignore="TouchTargetSizeCheck" />

                <Spinner
                    android:id="@+id/spinner_today2"
                    android:layout_width="90dp"
                    android:layout_height="28dp"

                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/convert_spinner_arrow3"
                    android:popupBackground="@drawable/convert_spinner_arrow3"
                    app:layout_constraintBottom_toBottomOf="@+id/textView_usageCharacter"
                    app:layout_constraintEnd_toStartOf="@+id/button_help2"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageView
                    android:id="@+id/imageView_usageCharacter"
                    android:layout_width="0dp"
                    android:layout_height="390dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time_image"
                    android:padding="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_usageCharacter"
                    app:srcCompat="@drawable/image_usage_character" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_bestUsedApp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/layout_usageCharacter">

                <TextView
                    android:id="@+id/textView_bestUsedApp"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="많이 사용한 앱"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Spinner
                    android:id="@+id/spinner_today3"
                    android:layout_width="70dp"
                    android:layout_height="28dp"

                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/convert_spinner_arrow3"
                    android:popupBackground="@drawable/convert_spinner_arrow3"
                    app:layout_constraintBottom_toBottomOf="@+id/textView_bestUsedApp"
                    app:layout_constraintEnd_toEndOf="@id/textView_bestUsedApp"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageView
                    android:id="@+id/imageView_bestUsedApp"
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_best_used_app_image"
                    android:padding="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_bestUsedApp"
                    app:srcCompat="@drawable/image_best_used_app" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_bestUsedWeb"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/layout_bestUsedApp">

                <TextView
                    android:id="@+id/textView_bestUsedWeb"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="많이 사용한 웹사이트"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Spinner
                    android:id="@+id/spinner_today4"
                    android:layout_width="70dp"
                    android:layout_height="28dp"

                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/convert_spinner_arrow3"
                    android:popupBackground="@drawable/convert_spinner_arrow3"
                    app:layout_constraintBottom_toBottomOf="@+id/textView_bestUsedWeb"
                    app:layout_constraintEnd_toEndOf="@id/textView_bestUsedWeb"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time_image"
                    android:gravity="center_horizontal"
                    android:paddingTop="55dp"
                    android:text="데이터 없음"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_bestUsedWeb" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_recentlyPhoneCall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/layout_bestUsedWeb">

                <TextView
                    android:id="@+id/textView_recentlyPhoneCall"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="최근 통화/문자"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/button_more"
                    android:layout_width="70dp"
                    android:layout_height="28dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="4dp"
                    android:background="@drawable/custom_button3"
                    android:drawableRight="@drawable/ic_more"
                    android:foregroundGravity="bottom"
                    android:includeFontPadding="false"
                    android:insetLeft="0dp"
                    android:insetTop="0dp"
                    android:insetRight="0dp"
                    android:insetBottom="0dp"
                    android:minWidth="0dp"
                    android:minHeight="0dp"
                    android:outlineProvider="none"
                    android:paddingLeft="7dp"
                    android:paddingTop="0dp"
                    android:paddingRight="7dp"
                    android:paddingBottom="0dp"
                    android:text="더보기"
                    android:textColor="@color/black"
                    android:textSize="13sp"
                    app:iconGravity="end"
                    app:layout_constraintBottom_toBottomOf="@id/textView_recentlyPhoneCall"
                    app:layout_constraintEnd_toEndOf="@+id/textView_recentlyPhoneCall"
                    tools:ignore="TouchTargetSizeCheck" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="0dp"
                    android:layout_height="140dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time_image"
                    android:gravity="center_horizontal"
                    android:paddingTop="55dp"
                    android:text="데이터 없음"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_recentlyPhoneCall" />

                <TextView
                    android:id="@+id/textView_more"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginBottom="10dp"
                    android:drawableLeft="@drawable/ic_reference_mark"
                    android:text="  전화 문자 이력이 안 보이나요? 알아보기"
                    android:textColor="@color/black"
                    app:layout_constraintBottom_toBottomOf="@+id/textView12"
                    app:layout_constraintStart_toStartOf="@+id/textView12" />

            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/item2_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/back_item1"
            android:paddingBottom="30dp"
            android:visibility="invisible">

            <TextView
                android:id="@+id/textView2_2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="활동 타임라인"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView7_2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:text="자녀의 스마트기기에서 최근 어떤 활동이 일어 났는지 시간순서대로 나열합니다."
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView2_2" />

            <TextView
                android:id="@+id/textView8_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:paddingStart="10dp"
                android:paddingEnd="5dp"
                android:text="조회 시작일 :"
                android:textColor="@color/black"
                android:textStyle="normal|bold"
                app:layout_constraintEnd_toStartOf="@id/spinner_user2_2"
                app:layout_constraintTop_toTopOf="@id/spinner_user2_2" />

            <Spinner
                android:id="@+id/spinner_user2_2"
                android:layout_width="180dp"
                android:layout_height="35dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/convert_spinner_arrow2"
                android:contentDescription="11월15일,2021 - 11월15일,2021"
                android:popupBackground="@drawable/convert_spinner_arrow2"

                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7_2"
                tools:ignore="TouchTargetSizeCheck" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout_usageTime_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/spinner_user2_2">

                <TextView
                    android:id="@+id/textView_usageTime_2"
                    android:layout_width="0dp"
                    android:layout_height="70dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/layerlist_usage_time"
                    android:gravity="left|top"
                    android:paddingStart="10dp"
                    android:paddingTop="10dp"
                    android:text="타임라인"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/transparent"
                    android:orientation="horizontal"
                    android:paddingStart="5dp"
                    android:paddingEnd="5dp"
                    app:layout_constraintBottom_toBottomOf="@id/textView_usageTime_2"
                    app:layout_constraintStart_toStartOf="@id/textView_usageTime_2">


                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine1"
                        android:layout_width="65dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/custom_button4_left"
                        android:text="모든활동"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine2"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-0.5dp"
                        android:background="@drawable/custom_button4_hor"
                        android:text="앱"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine3"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="-0.5dp"
                        android:background="@drawable/custom_button4_hor"
                        android:text="웹"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/button_timeLine4"
                        android:layout_width="40dp"
